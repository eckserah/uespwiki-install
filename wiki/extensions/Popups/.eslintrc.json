{
	"extends": "wikimedia",
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": "5"
	},
	"env": {
		"browser": true,
		"jquery": true,
		"commonjs": true,
		"qunit": true
	},
	"globals": {
		"mediaWiki": false,
		"OO": false,
		"moment": false,
		"Redux": false,
		"ReduxThunk": false
	},
	"rules": {
		"dot-notation": [ "error", { "allowKeywords": true } ],
		"no-use-before-define": 0
	}
}
